      "feature": "disposableEmail",
      "operator": "eq",
      "value": 1,
      "remark": "用户是一次性邮箱"
    },
    {
      "rule_id": "RELATED-001",
      "feature": "invitedUserPast24hCnt",
      "operator": "gt",
      "value": 2,
      "remark": "用户的邀请人下24小时内出金大于2次"
    },
    {
      "rule_id": "RELATED-002",
      "feature": "invitedUserBanRatio",
